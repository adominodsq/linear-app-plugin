# Linear API credentials for integration tests
# Copy this file to .env and fill in your values

# Your Linear API key (get from Linear Settings > API)
LINEAR_API_KEY=lin_api_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# The team key (e.g., "ENG", "DIS", "PROD")
LINEAR_TEAM_KEY=YOUR_TEAM_KEY

# Your Linear workspace ID
LINEAR_WORKSPACE_ID=your-workspace-id
